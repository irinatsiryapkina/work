			</aux>
			<aux name="outflux shares by destination">
				<dimensions>
					<dim name="Country"/>
				</dimensions>
				<eqn>IF total_expected_demand &gt; 0 
THEN outflux_by_destination / total_expected_demand
ELSE 0</eqn>
